<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n" />
		</Console>
	</Appenders>
	<Loggers>
		<Logger name="org.apache.commons.httpclient" level="FATAL"/>
		<Logger name="httpclient.wire.header" level="FATAL"/>
		<Logger name="httpclient.wire.content" level="FATAL"/>
		<Logger name="org.apache.http.wire" level="FATAL"/>
		<Logger name="org.apache.http.headers" level="FATAL"/>
		<Logger name="org.apache.http.impl.conn.PoolingHttpClientConnectionManager" level="FATAL"/>
		<Logger name="org.apache.http.client.protocol.RequestAddCookies" level="FATAL"/>
		<Logger name="org.apache.http.client.protocol.RequestAuthCache" level="FATAL"/>
		<Logger name="org.apache.http.impl.execchain.MainClientExec" level="FATAL"/>
		<Root level="debug">
			<AppenderRef ref="Console" />
		</Root>
	</Loggers>
</Configuration>